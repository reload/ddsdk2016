{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "drmonty/chosen",
                "version": "1.6.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.6.1/chosen_v1.6.1.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "type": "drupal-library",
                "name": "woothemes/flexslider",
                "version": "2.6.3",
                "source": {
                    "type": "git",
                    "url": "https://github.com/woothemes/FlexSlider",
                    "reference": "2.6.3"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.6.0",
        "cweagans/composer-patches": "^1.6.5",
        "drmonty/chosen": "^1.6.1",
        "drupal-composer/drupal-scaffold": "^2.5.4",
        "drupal/address": "^1.4.0",
        "drupal/admin_toolbar": "^1.25.0",
        "drupal/better_exposed_filters": "^3.0.0-alpha5",
        "drupal/chosen": "^2.6.0",
        "drupal/chosen_lib": "^2.6.0",
        "drupal/cookieconsent": "^1.4.0",
        "drupal/core": "^8.6.10",
        "drupal/crop": "^1.5.0",
        "drupal/ctools": "^3.0.0",
        "drupal/dropguard": "^1.2.0",
        "drupal/editor_advanced_link": "^1.4.0",
        "drupal/entity": "^1.0.0-rc1",
        "drupal/entity_reference_revisions": "^1.6.0",
        "drupal/extlink": "^1.1.0",
        "drupal/facets": "^1.2.0",
        "drupal/field_group": "^1.0.0",
        "drupal/field_redirection": "2.x-dev",
        "drupal/forward": "^2.1.0",
        "drupal/google_analytics": "^2.3.0",
        "drupal/http2_server_push": "^1.0.0",
        "drupal/image_widget_crop": "^2.2.0",
        "drupal/inline_entity_form": "^1.0.0-rc1",
        "drupal/link_attributes": "^1.5.0",
        "drupal/maxlength": "^1.0.0-beta2",
        "drupal/media_entity": "^1.8.0",
        "drupal/media_entity_image": "^1.3.0",
        "drupal/metatag": "^1.8.0",
        "drupal/monitoring": "^1.7.0",
        "drupal/paragraphs": "^1.6.0",
        "drupal/pathauto": "^1.3.0",
        "drupal/redirect": "^1.3.0",
        "drupal/reloadtina": "^1.0.0-alpha1",
        "drupal/scheduler": "^1.0",
        "drupal/search_api": "^1.11.0",
        "drupal/search_api_exclude_entity": "^1.0.0-beta2",
        "drupal/search_api_solr": "^2.5.0",
        "drupal/stage_file_proxy": "^1.0.0-alpha3",
        "drupal/system_status": "^2.8.0",
        "drupal/taxonomy_access_fix": "^2.6.0",
        "drupal/token": "^1.5.0",
        "drupal/twig_xdebug": "^1.0.0",
        "drupal/video_embed_field": "^1.5.0",
        "drush/drush": "^8.1.15",
        "woothemes/flexslider": "^2.6.3",
        "drupal/monolog": "^1.0.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3",
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "phpcs": "vendor/bin/phpcs --standard=vendor/drupal/coder/coder_sniffer/Drupal/ruleset.xml,phpcs.xml",
        "phpcs-checkstyle": "vendor/bin/phpcs --standard=vendor/drupal/coder/coder_sniffer/Drupal/ruleset.xml,phpcs.xml --report=checkstyle --report-file=tests/reports/phpcs-checkstyle.xml"
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/core": {
                "2558207: Add hook to react to image_path_flush() - recommended by the Reloadtina module": "https://www.drupal.org/files/issues/add_hook_image_path_flush-2558207-4.patch",
                "2408549: Notify about overridden configuration": "https://www.drupal.org/files/issues/2408549-106.patch",
                "2857132: Account administration pages language negotiation ignores _format blocking REST resources": "https://www.drupal.org/files/issues/2019-01-28/preserve-format-while-negoatiating-2857132-10-work-in-progress.patch"
            },
            "drupal/monolog": {
                "formatting": "https://www.drupal.org/files/issues/2018-12-11/3019715-2.patch"
            }
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "composer-exit-on-patch-failure": true
    }
}
