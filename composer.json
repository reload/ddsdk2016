{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "drmonty/chosen",
                "version": "1.6.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.6.1/chosen_v1.6.1.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "type": "drupal-library",
                "name": "woothemes/flexslider",
                "version": "2.6.3",
                "source": {
                    "type": "git",
                    "url": "https://github.com/woothemes/FlexSlider",
                    "reference": "2.6.3"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.0.20",
        "cweagans/composer-patches": "^1.6.5",
        "drmonty/chosen": "~1.6",
        "drupal-composer/drupal-scaffold": "^2.0.1",
        "drupal/address": "^1.1",
        "drupal/admin_toolbar": "^1.19",
        "drupal/better_exposed_filters": "~3.0",
        "drupal/chosen": "^2.2",
        "drupal/console": "~1.0",
        "drupal/core": "^8.4.7",
        "drupal/dropguard": "~1.0",
        "drupal/editor_advanced_link": "~1.0",
        "drupal/entity": "^1.0",
        "drupal/entity_reference_revisions": "~1.2",
        "drupal/extlink": "~1.0",
        "drupal/facets": "~1.0",
        "drupal/field_group": "~1.0",
        "drupal/forward": "~2.0",
        "drupal/google_analytics": "^2.1",
        "drupal/http2_server_push": "~1.0",
        "drupal/image_widget_crop": "~2.0",
        "drupal/inline_entity_form": "~1.0",
        "drupal/link_attributes": "~1.0",
        "drupal/maxlength": "~1.0",
        "drupal/media_entity": "~1.0",
        "drupal/media_entity_image": "~1.0",
        "drupal/metatag": "^1.2",
        "drupal/monitoring": "^1.4",
        "drupal/paragraphs": "^1.1",
        "drupal/pathauto": "~1.0",
        "drupal/redirect": "~1.0",
        "drupal/reloadtina": "~1.0",
        "drupal/search_api": "^1.3",
        "drupal/search_api_solr": "^1.0@beta",
        "drupal/stage_file_proxy": "~1.0",
        "drupal/system_status": "~2.0",
        "drupal/taxonomy_access_fix": "~2.0",
        "drupal/twig_xdebug": "~1.0",
        "drupal/video_embed_field": "~1.0",
        "drush/drush": "~8.0",
        "solarium/solarium": "3.8.*",
        "woothemes/flexslider": "~2.6",
        "drupal/search_api_exclude_entity": "^1.0",
        "drupal/field_redirection": "2.x-dev",
        "drupal/cookieconsent": "^1.3"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8",
        "drupal/coder": "^8.2",
        "squizlabs/php_codesniffer": "^2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "phpcs": "vendor/bin/phpcs --standard=vendor/drupal/coder/coder_sniffer/Drupal/ruleset.xml,phpcs.xml",
        "phpcs-checkstyle": "vendor/bin/phpcs --standard=vendor/drupal/coder/coder_sniffer/Drupal/ruleset.xml,phpcs.xml --report=checkstyle --report-file=tests/reports/phpcs-checkstyle.xml"
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/core": {
                "SA-CORE-2018-006": "https://gist.githubusercontent.com/reload-deploy/a5237b38ec73aeca7de667bf59fbbcbf/raw/768bbd3f46ab87d10e90d291c491373d93e690d5/SA-CORE-2018-006_8-4.patch",
                "2558207: Add hook to react to image_path_flush() - recommended by the Reloadtina module": "https://www.drupal.org/files/issues/add_hook_image_path_flush-2558207-4.patch",
                "2408549: Notify about overridden configuration": "https://www.drupal.org/files/issues/2408549-91.patch",
                "2857132: Account administration pages language negotiation ignores _format blocking REST resources": "https://www.drupal.org/files/issues/preserve-format-while-negoatiating-2857132.patch",
                "2867834: Backwards variables in Install profile validation error message": "https://www.drupal.org/files/issues/backwards_variables_in_message-2867834-25.patch"
	    },
            "drupal/chosen": {
                "2705891: Fix invisible required chosen widgets": "https://www.drupal.org/files/issues/2705891-chosen-validation-temp-fix-6.patch"
            }
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "composer-exit-on-patch-failure": true
    }
}
